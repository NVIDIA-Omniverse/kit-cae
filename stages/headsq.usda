def Xform "World"
{
    def Xform "headsq"
    {
        def Scope "headsq_vti"
        {
            def CaeDataSet "VTIDataSet" (
                prepend apiSchemas = ["CaeDenseVolumeAPI"]
            )
            {
                uniform int3 cae:denseVolume:maxExtent = (255, 255, 93)
                uniform int3 cae:denseVolume:minExtent = (0, 0, 0)
                uniform float3 cae:denseVolume:spacing = (1, 1, 2)
                custom rel field:Scalars_ = </World/headsq/headsq_vti/PointData/Scalars_>
            }

            class "VTKFieldArrayClass"
            {
                asset[] fileNames = [@../data/headsq.vti@]
            }

            def Scope "PointData"
            {
                def CaeVtkFieldArray "Scalars_" (
                    specializes = </World/headsq/headsq_vti/VTKFieldArrayClass>
                )
                {
                    string arrayName = "Scalars_"
                    uniform token fieldAssociation = "vertex"
                }
            }
        }
    }
}