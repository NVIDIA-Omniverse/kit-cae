def Xform "Root"
{
    def Xform "StaticMixer_tet"
    {
        def Scope "StaticMixer_tet_npz"
        {
            class "NumPyFieldArrayClass"
            {
                bool allowPickle = 0
                uniform token fieldAssociation
                asset[] fileNames = [@StaticMixer_tet.npz@]
            }

            def CaeDataSet "NumPyMesh" (
                apiSchemas = ["CaeSidsUnstructuredAPI"]
            )
            {
                rel cae:sids:elementConnectivity = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/element_connectivity>
                rel cae:sids:elementStartOffset
                uniform token cae:sids:elementType = "TETRA_4"
                rel cae:sids:gridCoordinates = [
                    </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/coords_0>,
                    </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/coords_1>,
                    </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/coords_2>,
                ]
                custom rel field:eddyViscosity = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/Eddy_Viscosity>
                custom rel field:pressure = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/Pressure>
                custom rel field:temperature = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/Temperature>
                custom rel field:totalPressure = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/Total_Pressure>
                custom rel field:totalTemperature = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/Total_Temperature>
                custom rel field:turbulenceEddyDissipation = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/Turbulence_Eddy_Dissipation>
                custom rel field:turbulenceKineticEnergy = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/Turbulence_Kinetic_Energy>
                custom rel field:velocity0 = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/Velocity_0>
                custom rel field:velocity1 = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/Velocity_1>
                custom rel field:velocity2 = </Root/StaticMixer_tet/StaticMixer_tet_npz/NumPyArrays/Velocity_2>
            }

            def Scope "NumPyArrays"
            {
                def CaeNumPyFieldArray "coords_0"
                {
                    bool allowPickle = 0
                    string arrayName = "coords_0"
                    uniform token fieldAssociation
                    asset[] fileNames = [StaticMixer_tet.npz@]
                }

                def CaeNumPyFieldArray "coords_1"
                {
                    bool allowPickle = 0
